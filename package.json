  "description": "This is a simple wrapper project for building fabricate (fab.ts.adligo.org).",
  "dependencies": {
    "child_process": "^1.0.2",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.12"
  },
  "homepage": "https://github.com/adligo/fab_group.ts.adligo.org#readme",
  "keywords": [
    "ant",
    "build",
    "fabricate",
    "fab",
    "gradle",
