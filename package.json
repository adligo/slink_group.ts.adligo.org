    "maven",
    "typescript"
  ],
  "license": "Apache-2.0",
  "name": "@ts.adligo.org/fab_group",
  "main": "src/fab.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adligo/fab_group.ts.adligo.org.git"
  },
  "scripts": {
    "build": "node --disable-warning=DEP0190 buildSrc/build.cjs",
    "git-clone": "node --disable-warning=DEP0190 buildSrc/git-clone-or-pull.cjs",
    "git-clone-or-pull": "node --disable-warning=DEP0190 buildSrc/git-clone-or-pull.cjs --pull",
    "git-clone-local": "node --disable-warning=DEP0190 buildSrc/git-clone-local.cjs",
    "git-clone-ssh": "node --disable-warning=DEP0190 buildSrc/git-clone-or-pull.cjs --ssh",
    "setup": "node --disable-warning=DEP0190 buildSrc/setup.cjs",
    "tests": "node --disable-warning=DEP0190 buildSrc/tests.cjs && node buildSrc/publishTestResults.cjs",
    "rm": "node --disable-warning=DEP0190 buildSrc/rm.cjs",
    "rmNm": "node --disable-warning=DEP0190 buildSrc/rmNm.cjs",
    "tsc": "tsc"
  },
  
  "type": "module",
  "version": "1.0.7"
